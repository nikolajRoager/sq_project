set terminal pdf
set size ratio 1.0
set output 'solenoid_euler_yz_view.pdf'
set xlabel 'y/m'
set ylabel 'z/m'
set key out
set  title 'Euler method, front-view'
plot [-1:1][-1:1] 'out/out_euler_timesteps/particles0.tsv' index 0 using 3:4 with lines  title '$h=0.01 \mu s$',\
'out/out_euler_timesteps/particles31.tsv' index 0 using 3:4 with lines  title '$h=0.32 \mu s$',\
'out/out_euler_timesteps/particles63.tsv' index 0 using 3:4 with lines  title '$h=0.64 \mu s$',\
'out/out_euler_timesteps/particles95.tsv' index 0 using 3:4 with lines  title '$h=0.96 \mu s$',\
'out/out_euler_timesteps/particles127.tsv' index 0 using 3:4 with lines  title '$h=1.28 \mu s$'



set  title 'RK4, front-view'
set output 'solenoid_RK_yz_view.pdf'
plot [-1:1][-1:1] 'out/out_RK4_timesteps/particles0.tsv' index 0 using 3:4 with lines  title '$h=0.01 \mu s$',\
'out/out_RK4_timesteps/particles31.tsv' index 0 using 3:4 with lines  title '$h=0.32 \mu s$',\
'out/out_RK4_timesteps/particles63.tsv' index 0 using 3:4 with lines  title '$h=0.64 \mu s$',\
'out/out_RK4_timesteps/particles95.tsv' index 0 using 3:4 with lines  title '$h=0.96 \mu s$',\
'out/out_RK4_timesteps/particles127.tsv' index 0 using 3:4 with lines  title '$h=1.28 \mu s$'


set output 'solenoid_RK4_Xt.pdf'
set ylabel 'x error $/x_{an}$'
set xlabel '$t/\mu s$'
set  title 'Runge Kutta 4 method, V relative error'
plot [][-1:1] 'out/out_RK4_timesteps/particles0.tsv' index 0 using 1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=0.01 \mu s$',\
'out/out_RK4_timesteps/particles31.tsv' index 0 using  1:($2/($1*0.1597577737816823-2.5)-1)  with lines  title '$h=0.32 \mu s$',\
'out/out_RK4_timesteps/particles63.tsv' index 0 using  1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=0.64 \mu s$',\
'out/out_RK4_timesteps/particles95.tsv' index 0 using  1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=0.96 \mu s$',\
'out/out_RK4_timesteps/particles127.tsv' index 0 using 1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=1.28 \mu s$'



set output 'solenoid_RK4_Xt.pdf'
set ylabel 'x error $/x_{an}$'
set xlabel '$t/\mu s$'
set  title 'Runge Kutta 4 method, x(t) relative error'
plot [][-0.1:0.1] 'out/out_RK4_timesteps/particles0.tsv' index 0 using 1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=0.01 \mu s$',\
'out/out_RK4_timesteps/particles31.tsv' index 0 using  1:($2/($1*0.1597577737816823-2.5)-1)  with lines  title '$h=0.32 \mu s$',\
'out/out_RK4_timesteps/particles63.tsv' index 0 using  1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=0.64 \mu s$',\
'out/out_RK4_timesteps/particles95.tsv' index 0 using  1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=0.96 \mu s$',\
'out/out_RK4_timesteps/particles127.tsv' index 0 using 1:($2/($1*0.1597577737816823-2.5)-1) with lines  title '$h=1.28 \mu s$'


set output 'solenoid_RK4_Xt_abs.pdf'
set ylabel 'x error $/m$'
set xlabel '$t/\mu s$'
set  title 'Runge Kutta 4 method, x(t) error'
plot [][-0.1:0.1] 'out/out_RK4_timesteps/particles0.tsv' index 0 using 1:($2-$1*0.1597577737816823+2.5) with lines  title '$h=0.01 \mu s$',\
'out/out_RK4_timesteps/particles31.tsv' index 0 using  1:($2-$1*0.1597577737816823+2.5)  with lines  title '$h=0.32 \mu s$',\
